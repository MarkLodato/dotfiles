include "%L"
include "%H/.pointless-xcompose/xcompose"

# Things that are not included above.
<Multi_key> <minus> <minus> <greater>   : "⟶"   U27F6   # LONG RIGHTWARDS ARROW
<Multi_key> <minus> <minus> <less>      : "⟵"   U27F5   # LONG LEFTWARDS ARROW

# Aliases that I can actually remember.
<Multi_key> <e> <u> <r>                 : "€"           # EURO SYMBOL
<Multi_key> <l> <b>                     : "£"           # POUND SYMBOL
